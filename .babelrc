{
  "presets": [
    ["env",{
      "target":{
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}